{
  "name": "@faqapp/core",
  "version": "0.1.0",
  "description": "Core SDK for thefaqapp - FAQ management and SEO optimization",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okbrkk/thefaq-core.git"
  },
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "npm run build"
  },
  "keywords": [
    "faq",
    "seo",
    "content-management",
    "thefaqapp"
  ],
  "author": "thefaqapp",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.2.1",
    "msw": "^2.1.5",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.2.1"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/okbrkk/thefaq-core/issues"
  },
  "homepage": "https://github.com/okbrkk/thefaq-core#readme"
}